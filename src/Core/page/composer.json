{
    "name": "Core/page",
    "description": "Page package for Core",
    "keywords": ["Laravel", "page"],
    "license": "MIT",
    "authors": [
        {
            "name": "Laravel",
            "email": "info@laravel.org"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "composer/installers": "~1.4"
    },
    "autoload": {
        "classmap": [
            "database/seeds"
        ],
        "psr-4": {
            "Core\\Page\\": "/src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Core\\Page\\PageServiceProvider"
            ],
            "aliases": {
                "Page"       : "Core\\Page\\Facades\\Page"
            }
        }
    }
}
